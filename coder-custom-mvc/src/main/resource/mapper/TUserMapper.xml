<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coder.mybatis.mapper.IUserMapper">
    <select id="selectByPrimaryKey" resultType="com.coder.mybatis.entity.User">
          select * from STUDENT t where t.id='1'
    </select>

    <select id="selectAll" resultType="com.coder.mybatis.entity.Sale">
          select * from SALES_TAB
    </select>

</mapper>